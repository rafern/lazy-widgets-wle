  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "@wonderlandengine/api": "^1.0.0-rc.5",
    "@wonderlandengine/components": "link:../../Contrib/components",
    "eslint": "^8.32.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "gl-matrix": "^3.4.3",
    "lazy-widgets": "file:lazy-widgets-0.6.0.tgz",
    "shx": "^0.3.4",
    "tslib": "^2.4.1",
    "typedoc": "^0.23.24",
    "typedoc-plugin-external-resolver": "^1.0.1",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "@wonderlandengine/api": ">= 1.0.0-rc.5",
    "@wonderlandengine/components": ">= 1.0.0-rc.6",
    "gl-matrix": "3.x",
    "lazy-widgets": ">= 0.6 < 1"
  }
